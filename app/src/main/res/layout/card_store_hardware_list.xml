        app:layout_constraintBottom_toBottomOf="@+id/imageView47"
        app:layout_constraintStart_toEndOf="@+id/imageView47"
        app:layout_constraintTop_toTopOf="@+id/imageView47"
        app:layout_constraintVertical_bias="0.0" />

    <TextView
        android:id="@+id/textView187"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:layout_marginEnd="8dp"
        android:text="2.4G/5G"
        app:layout_constraintBottom_toBottomOf="@+id/textView186"
        app:layout_constraintEnd_toStartOf="@+id/textView188"
        app:layout_constraintStart_toEndOf="@+id/textView186"
        app:layout_constraintTop_toTopOf="@+id/textView186" />

    <TextView
        android:id="@+id/textView188"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="æœ‰"
        android:textColor="@color/colorGreen"
        app:layout_constraintBottom_toBottomOf="@+id/textView187"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="@+id/textView187" />
</android.support.constraint.ConstraintLayout>

